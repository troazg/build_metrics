<div class="card-deck mb-3 text-center">
  {{#each builds}}
    <div class="card build-card">
      <h5 class="card-title">
        <a href="/builds/{{_id}}">{{name}}</a>
      </h5>
      <p class="card-text">
        <p>Build Health: <span id="{{_id}}_health">Poor</span></p>
        <p>Last status: <span id="{{_id}}_status">Failed</span></p>
      </p>
    </div>
  {{/each}}
</div>

